pipeline {
    agent any

   stage('Clone Repository') {
    steps {
        git branch: 'main', 
            credentialsId: 'jeevan', 
            url: 'https://github.com/sandeepvilla1612/sandeep.git'
    }
}

        stage('Deploy to Apache') {
            steps {
                script {
                    sh 'sudo cp -r * /var/www/html/'
                }
            }
        }
    }
}

